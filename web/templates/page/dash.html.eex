<h1>PRs Involving You</h1>

<ol>
  <%= for {repo, prs} <- @prs_involving_you do %>
      <h2><%= repo %></h2>
      <%= for pr <- prs do %>
          <% title = pr["title"] %>
          <li>
            <a href='<%= pr["html_url"] %>'>
              <%= title %>
            </a>
          </li>
      <% end %>
  <% end %>
</ol>
